<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Overview">

    <TextView
        android:id="@+id/listownerName3"
        android:layout_width="412dp"
        android:layout_height="105dp"
        android:background="#959393"
        android:gravity="center"
        android:text="Your Lists"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/buttonList1"
        android:layout_width="392dp"
        android:layout_height="175dp"
        android:text="Home Groceries\nJohn Smith"
        android:paddingTop="70dp"
        app:layout_constraintTop_toBottomOf="@+id/listownerName3"
        tools:layout_editor_absoluteX="6dp" />

    <Button
        android:id="@+id/buttonList2"
        android:layout_width="392dp"
        android:layout_height="175dp"
        android:text="Dinner Supplies\nJohn Smith"
        android:paddingTop="70dp"
        app:layout_constraintTop_toBottomOf="@+id/buttonList1"
        tools:layout_editor_absoluteX="9dp" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="69dp"
        android:layout_height="73dp"
        tools:layout_editor_absoluteX="26dp"
        tools:layout_editor_absoluteY="16dp"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="51dp"
        android:layout_height="48dp"
        android:translationZ="10"
        tools:layout_editor_absoluteX="180dp"
        tools:layout_editor_absoluteY="138dp"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="51dp"
        android:layout_height="48dp"
        android:translationZ="10"
        tools:layout_editor_absoluteX="180dp"
        tools:layout_editor_absoluteY="312dp"
        tools:srcCompat="@tools:sample/avatars" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/floatingActionButton3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        app:srcCompat="@android:drawable/ic_input_add"
        tools:layout_editor_absoluteX="330dp"
        tools:layout_editor_absoluteY="647dp" />

</androidx.constraintlayout.widget.ConstraintLayout>