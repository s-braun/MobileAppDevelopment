<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Overview">

    <TextView
        android:id="@+id/listownerName3"
        android:layout_width="412dp"
        android:layout_height="105dp"
        android:background="#959393"
        android:gravity="center"
        android:text="Your Lists"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/buttonList1"
        android:layout_width="392dp"
        android:layout_height="175dp"
        android:paddingTop="70dp"
        android:text="Home Groceries\nJohn Smith"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.473"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/listownerName3"
        app:layout_constraintVertical_bias="0.028" />

    <Button
        android:id="@+id/buttonList2"
        android:layout_width="392dp"
        android:layout_height="175dp"
        android:paddingTop="70dp"
        android:text="Dinner Supplies\nJohn Smith"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.473"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonList1"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="69dp"
        android:layout_height="73dp"
        app:layout_constraintBottom_toBottomOf="@+id/listownerName3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.093"
        app:layout_constraintStart_toStartOf="@+id/listownerName3"
        app:layout_constraintTop_toTopOf="@+id/listownerName3"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="51dp"
        android:layout_height="48dp"
        android:translationZ="10dp"
        app:layout_constraintBottom_toBottomOf="@+id/buttonList1"
        app:layout_constraintEnd_toEndOf="@+id/buttonList1"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintStart_toStartOf="@+id/buttonList1"
        app:layout_constraintTop_toTopOf="@+id/buttonList1"
        app:layout_constraintVertical_bias="0.196"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="51dp"
        android:layout_height="48dp"
        android:translationZ="10dp"
        app:layout_constraintBottom_toBottomOf="@+id/buttonList2"
        app:layout_constraintEnd_toEndOf="@+id/buttonList2"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintStart_toStartOf="@+id/buttonList2"
        app:layout_constraintTop_toBottomOf="@+id/buttonList1"
        app:layout_constraintVertical_bias="0.196"
        tools:srcCompat="@tools:sample/avatars" />

    <Button
        android:id="@+id/button2"
        android:layout_width="401dp"
        android:layout_height="87dp"
        android:drawableLeft="@android:drawable/ic_input_add"
        android:paddingLeft="50dp"
        android:paddingRight="50dp"
        android:text="Create a new shopping list"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonList2"
        app:layout_constraintVertical_bias="0.909" />

</androidx.constraintlayout.widget.ConstraintLayout>